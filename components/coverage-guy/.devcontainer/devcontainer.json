{
  "name": "Coverage-Guy Dev Container",
  "runArgs": ["--privileged", "--cpus=10"],
  "build": {
    "dockerfile": "../Dockerfile",
    "context": "../"
  },
  "workspaceMount": "source=${localWorkspaceFolder}/,target=/shellphish/coverageguy,type=bind,consistency=cached",
  "workspaceFolder": "/shellphish/coverageguy",
  "mounts": [
    "source=${localWorkspaceFolder}/../../libs/,target=/shellphish/libs,type=bind,consistency=cached",
    "source=/aixcc-backups/,target=/aixcc-backups,type=bind,consistency=cached",
    "source=/shared/,target=/shared,type=bind,consistency=cached",
    "source=/var/run/docker.sock,target=/var/run/docker.sock,type=bind"
  ],
  "customizations": {
    "env": {
    },
    "vscode": {
      "extensions": [
        "github.copilot",
        "ms-azuretools.vscode-docker",
        "ms-vscode.vscode-remote-extensionpack",
        "ms-python.python"
      ],
      "settings": {
        "terminal.integrated.shell.linux": "/bin/bash"
      }
    }
  },
  "forwardPorts": [
    8050,
    8080
  ]
}